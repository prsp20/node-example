{
  "name": "node-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "---- build ----": "",
    "dev": "nodemon src/server.js --exec babel-node",
    "build": "rimraf lib && babel src --out-dir lib --copy-files --ignore spec.js,test.js,mock.js",
    "start": "yarn build && node lib/server.js",
    "---- eslint ----": "",
    "eslint:watch": "esw -w --color --cache --fix",
    "eslint": "eslint .",
    "---- prepush hooks ----": "",
    "prepush": "yarn eslint && yarn test",
    "---- test----": "",
    "test": "jest --coverage --runInBand",
    "test:watch": "jest --watch --coverage"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "debug": "~2.6.9",
    "express": "^4.16.2",
    "express-correlation-id": "^1.0.2",
    "express-healthcheck": "^0.1.0",
    "lodash": "^4.17.4",
    "superagent": "^3.6.3",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "4.4.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-watch": "^3.1.2",
    "get-port": "^3.2.0",
    "husky": "^0.14.3",
    "inmemory-mongo": "^0.0.2",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0"
  }
}
